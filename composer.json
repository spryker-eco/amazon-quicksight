{
    "name": "spryker-eco/amazon-quicksight",
    "type": "library",
    "description": "AmazonQuicksight module",
    "license": "proprietary",
    "require": {
        "aws/aws-sdk-php": "^3.90.0",
        "php": ">=8.1",
        "spryker/kernel": "^3.30.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerEco\\": "src/SprykerEco/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerEcoTest\\": "tests/SprykerEcoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "../../bin/phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "../../bin/phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
